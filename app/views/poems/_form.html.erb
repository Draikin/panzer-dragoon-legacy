<%= form_for @poem do |f| %>
  <%= render 'shared/error_messages', target: @poem %>
  <%= render 'shared/form/contributors', model: @poem, f: f %>
  <%= render 'shared/form/name', f: f %>
  <%= render 'shared/form/description', f: f %>
  <%= render 'shared/form/content', f: f %>
  <%= render 'shared/form/related_encyclopaedia_entries', model: @poem, f: f %>
  <%= render 'shared/form/publish', model: @poem, f: f %>
  <%= render 'shared/form/save_buttons', f: f %>
<% end %>
